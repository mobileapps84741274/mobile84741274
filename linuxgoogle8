/usr/bin/expect <<EOF
spawn su
expect "#"
send "vncserver -geometry 1600x900 :82\r"
expect "Password:"
send "$mypass\r"
expect "Verify:"
send "$mypass\r"
expect "(y/n)? "
send "n\r"
expect eof
exit
EOF

sleep 8

