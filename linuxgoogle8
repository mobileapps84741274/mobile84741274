myuser="root"
mypass="linux84"

/usr/bin/expect <<EOF
spawn su
expect "#"
send "vncserver -geometry 1600x900 :82\r"
expect "Password:"
send "$mypass\r"
expect "Verify:"
send "$mypass\r"
expect "(y/n)? "
send "n\r"
expect eof
exit
EOF

sleep 8

DISPLAY=:82 sudo xdotool mousemove 772 482 click 1

sleep 12

DISPLAY=:82 sudo bash /var/www/html/google844/google844 &> /dev/null &
