document.getElementsByClassName("sso-button__text-container")[0].click();

}, 8444);

setInterval(function() {

if(location.href.match(/notebook/))

{

if(document.getElementsByClassName("notebook-page-container__notebook-offline-banner")[0].innerText.match(/You are viewing the static version/))

{

location = "https://console.paperspace.com/notebooks";

}

else

{

location = "https://console.paperspace.com/notebooks";

}

}, 34884);

